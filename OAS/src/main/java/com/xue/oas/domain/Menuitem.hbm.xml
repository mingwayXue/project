<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xue.oas.domain.Menuitem">
		<id name="mid" length="5" column="mid">
			<generator class="assigned"></generator>
		</id>
		<property name="pid" column="pid" length="5"></property>
		<property name="isParent" column="isParent" type="java.lang.Boolean"></property>
		<property name="name" column="name" length="20"></property>
		<property name="icon" column="icon" length="100"></property>
	</class>
</hibernate-mapping>